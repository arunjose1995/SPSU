{
    "name": "ui",
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "preview": "vite preview",
        "lint-check": "eslint .",
        "lint-fix": "eslint --config eslint.config.js --fix .",
        "prettier-check": "prettier --check .",
        "prettier-fix": "prettier --write . --config ./.prettierrc",
        "format": "npm run prettier-fix && npm run lint-fix",
        "prepare": "husky"
    },
    "dependencies": {
        "@emotion/react": "11",
        "@emotion/styled": "11",
        "@mui/icons-material": "6",
        "@mui/material": "6",
        "@mui/material-nextjs": "6",
        "@toolpad/core": "0.12.0",
        "react": "19",
        "react-dom": "19",
        "react-router": "7"
    },
    "devDependencies": {
        "@types/react": "18",
        "@types/react-dom": "18",
        "@typescript-eslint/eslint-plugin": "^8.24.1",
        "@typescript-eslint/parser": "^8.24.1",
        "@vitejs/plugin-react": "4.3.2",
        "eslint": "^9.20.0",
        "eslint-plugin-prettier": "5.2.3",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "5.1.0",
        "eslint-plugin-react-refresh": "0.4.19",
        "globals": "15.15.0",
        "husky": "9.1.7",
        "lint-staged": "15.4.3",
        "prettier": "3.5.0",
        "typescript": "5.7.3",
        "typescript-eslint": "8.24.0",
        "vite": "5.4.8"
    },
    "lint-staged": {
        "*.js": [
            "eslint --cache --fix",
            "prettier --write"
        ],
        "*.jsx": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.json": [
            "prettier --write"
        ],
        "*.ts": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.tsx": [
            "eslint --fix",
            "prettier --write"
        ]
    }
}
